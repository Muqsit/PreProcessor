{
    "name": "muqsit/preprocessor",
    "description": "PHP library to ready code for production",
    "type": "library",
    "license": "LGPL-3.0-only",
    "require": {
        "php": "^8.1",
        "phpstan/phpstan": "1.11.*",
        "nikic/php-parser": "5.3.*",
        "symfony/filesystem": "~6.4.0",
        "php-parallel-lint/php-console-color": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "muqsit\\preprocessor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "muqsit\\preprocessor\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3",
        "pocketmine/pocketmine-mp": "5.6.0"
    }
}
